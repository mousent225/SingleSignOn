<script type="text/javascript">
    $(document).ready(function () {
        SetHeight($("#sub_content", 244));
        $("#contents").width($(window).width() - $("#left_wrap").width());
    });
    $(window).resize(function () {
        SetHeight($("#sub_content"), 244);
        $("#contents").width($(window).width() - $("#left_wrap").width());
    });
    function SetHeight(element, diff) {
        element.css({
            height: $(window).height() - diff,
            "max-height": $(window).height() - diff,
            overflow: "auto"
        });
    }
</script>