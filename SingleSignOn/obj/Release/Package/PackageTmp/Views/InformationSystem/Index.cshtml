@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<style type="text/css">
    ul.location {
        float: left;
        margin: 0;
    }

        ul.location li {
            display: inline;
        }

    a:link {
        color: #343434;
        text-decoration: none;
    }

    #sub_content {
        padding-left: 10px;
        width: auto;
    }
    
    #LblockPageHeader {
        height: 35px;
        margin: -10px 0 15px;
        position: relative;
    }

    #LblockPageTitle {
        height: 100%;
        background: rgba(0, 0, 0, 0) url("../Content/img/pageTitle_bg.gif") repeat-x scroll left bottom;
        margin-top: 14px;
    }

        #LblockPageTitle h1 {
            color: #377797;
            font-size: 16px;
            font-weight: bold;
            padding: 7px 0 0 5px;
        }

    #LblockMainBody {
        height: 469px;
        overflow: visible;
    }

    .myfos_type {
        border-left: 1px dotted #e8e8e8;
        float: left;
        padding: 5px 0 5px 25px;
        width: 30%;
    }

    .myfos_type_area {
        border: 1px solid #c2dbf9;
        overflow: hidden;
    }

        .myfos_type_area dl, dt, dd {
            margin: 0;
            padding: 0;
        }

    .myfos_type:first-child {
        border-left: 0 none;
        margin: 0;
    }


    .myfos_type DT {
        color: #222;
        font-weight: bold;
        padding: 0 0 10px;
    }

    .myfos_typeBg03 {
        position: relative;
        right: 0;
        top: 0;
    }

    .LblockApp table td {
        border: 1px solid #c2dbf9;
        line-height: 15px;
        padding: 2px;
    }

    .info_txt {
        color: #009;
        font-size: 12px;
        margin-bottom: 5px;
        margin-top: 5px;
    }

    .mifos_txt a {
        cursor: pointer;
        text-decoration: none;
        font-size: 12px;
    }
</style>

